{
    "name": "qian",
    "version": "2.0.0",
    "description": "A small file browser",
    "main": "main.js",
    "scripts": {
        "start": "electron main.js",
        "elm": "elm make src/Main.elm --output=src/js/elm_artifact.js",
        "build_darwin": "electron-packager . qian --platform=darwin --arch=x64 --out _build --overwrite --ignore='_releases|_build|elm\\-stuff'",
        "build_linux": "electron-packager . qian --platform=linux --arch=x64 --out _build --overwrite --ignore='_releases|_build|elm\\-stuff'",
        "build_win32": "electron-packager . qian --platform=win32 --arch=x64 --out _build --overwrite--ignore='_releases|_build|elm\\-stuff'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xvw/qian.git"
    },
    "keywords": [
        "explorer",
        "elm",
        "electron",
        "file"
    ],
    "author": "xvw <xaviervdw@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xvw/qian/issues"
    },
    "homepage": "https://github.com/xvw/qian#readme",
    "dependencies": {},
    "devDependencies": {
        "electron": "^3.0.2",
        "electron-packager": "^12.2.0",
        "elm": "^0.19.0-bugfix2"
    }
}
